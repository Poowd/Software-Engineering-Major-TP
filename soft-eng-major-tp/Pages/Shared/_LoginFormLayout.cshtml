<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - soft_eng_major_tp</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
</head>
<body>
    <main class="row w-100 p-0 m-0" style="height: 100vh">
        <section class="col-lg-6 m-0 p-0">
            @RenderBody()
        </section>
        <section class="col-lg-6 m-0 p-0">
            <img src="https://images.pexels.com/photos/3165335/pexels-photo-3165335.jpeg" alt="..." class="w-100 h-100" style="object-fit: cover" />
        </section>
    </main>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
